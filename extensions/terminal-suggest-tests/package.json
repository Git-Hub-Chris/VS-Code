{
  "name": "terminal-suggest-tests",
  "description": "Terminal Suggest tests for VS Code",
  "version": "0.0.1",
  "publisher": "vscode",
  "license": "MIT",
  "private": true,
  "activationEvents": [
    "onLanguage:json"
  ],
  "main": "./out/terminalSuggestMain",
  "engines": {
    "vscode": "*"
  },
  "icon": "media/icon.png",
  "scripts": {
    "vscode:prepublish": "node ../../node_modules/gulp/bin/gulp.js --gulpfile ../../build/gulpfile.extensions.js compile-extension:terminal-suggest-tests ./tsconfig.json",
    "watch": "gulp watch-extension:terminal-suggest-tests",
    "compile": "gulp compile-extension:terminal-suggest-tests"
  },
  "devDependencies": {
    "@types/node": "20.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/vscode.git"
  },
